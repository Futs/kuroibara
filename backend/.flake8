[flake8]
max-line-length = 88
extend-ignore = E203, E266, E501, W503, F403, F401, E402
max-complexity = 35
select = B,C,E,F,W,T4,B9
exclude = 
    .git,
    __pycache__,
    .venv,
    .tox,
    dist,
    build,
    *.egg,
    alembic/versions
per-file-ignores = 
    __init__.py:F401
    */tests/*:F401,F811
    */conftest.py:F401,F811
